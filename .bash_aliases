#
# Spoob aliases
#

# -----ls aliases-----

alias la='ls -a'
alias ll='ls -l'
alias lla='ls -la'
alias lal='ls -al'

alias ld='ls */'
alias lad='la */'
alias lda='la */'

# ---konsole aliases---

alias fork='nohup konsole > /dev/null 2>&1 & disown'

# -----git aliases-----

alias gcm='git commit -m'
alias gp='git push'
alias gst='git status'

